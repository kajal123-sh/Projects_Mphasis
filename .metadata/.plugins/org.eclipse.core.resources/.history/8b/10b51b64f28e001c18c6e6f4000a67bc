package com.simplilearn.operators;

public class Operators {

	public static void main(String[] args) {
		// Unary operator
		int a = 4;
		System.out.println(a++);
		System.out.println(a--);
		System.out.println(a);
		
		//Arithmrtic operator
		int b = 4;
		System.out.println(a+b);
		System.out.println(2%2);
		
		//Relational operator
		System.out.println(4<5);
		
		
	

	}

}
